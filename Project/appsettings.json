{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "PostgresDatabase": "Host=${DB_HOST};Port=${DB_PORT};Database=${DB_NAME};Username=${DB_USER};Password=${DB_PASS}"
  },
  "MinioDatabase": {
    "Hostname": "${MINIO_HOST}",
    "Port": "${MINIO_PORT}",
    "Username": "${MINIO_USERNAME}",
    "Password": "${MINIO_PASSWORD}"
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:4040"
      }
    }
  }
}
